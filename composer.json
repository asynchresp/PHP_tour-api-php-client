  "description": "PHP client library for Hotelspro Tours&Activities API",
  "homepage": "http://www.hotelspro.com",
  "keywords": [
    "metglobal",
    "hotelspro",
    "hotelspro.com",
    "hotelspro php",
    "hotelspro api",
    "hotelspro api php client",
    "hotelspro api php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Metglobal and contributors",
      "homepage": "https://github.com/metglobal/tour-api-php-client/contributors"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
